{
  "name": "next-paths",
  "version": "1.0.0",
  "description": "Generate path helpers for Next.js App Router",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "next-paths": "./dist/cli.js"
  },
  "scripts": {
    "build": "npm run clean && yarn generate:test && tsc",
    "prebuild": "npm run clean",
    "pretest": "yarn build",
    "test": "jest",
    "prepare": "npm run build",
    "generate": "node dist/generatePaths.js",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "generate:test": "tsx src/cli.ts generate --appDir tests/fixtures/app --outputDir tests/fixtures/app"
  },
  "keywords": [
    "nextjs",
    "typescript",
    "app-router",
    "paths",
    "routing",
    "next",
    "next.js",
    "app-directory"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "commander": "^11.0.0",
    "typescript": "^5.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@types/node": "^22.15.17",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "jest": "^29.0.0",
    "jest-environment-jsdom": "^29.7.0",
    "next": "^15.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-jest": "^29.0.0",
    "tsx": "^4.0.0"
  },
  "peerDependencies": {
    "next": ">=14.0.0",
    "react": ">=18.2.0",
    "react-dom": ">=18.2.0"
  },
  "files": [
    "dist/index.js",
    "dist/index.d.ts",
    "dist/generatePaths.js",
    "dist/generatePaths.d.ts",
    "dist/cli.js",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/paulostarek/next-paths.git"
  },
  "bugs": {
    "url": "https://github.com/paulostarek/next-paths/issues"
  },
  "homepage": "https://github.com/paulostarek/next-paths#readme",
  "packageManager": "yarn@1.22.21+sha1.1959a18351b811cdeedbd484a8f86c3cc3bbaf72",
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
